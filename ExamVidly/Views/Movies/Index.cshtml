@model IEnumerable<ExamVidly.Models.Movie>
	@{
		ViewBag.Title = "Index";
		Layout = "~/Views/Shared/_Layout.cshtml";
	}

	<h2>Movies</h2>

	<table class="table table-bordered table-hover">
		<thead>
			<tr>
				<th>Movie</th>
			</tr>
		</thead>
		<tbody>
			@foreach (var v in Model)
			{
				<tr>
					<td>
						@v.Name
					</td>
				</tr>
			}
		</tbody>
	</table>